{
  "name": "@honed/query-vue",
  "version": "0.0.1",
  "description": "A composable for handling query parameters for Vue and Inertia.",
  "keywords": [
    "honed",
    "vue",
    "inertia",
    "query-vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run format && vite build && vue-tsc --declaration --emitDeclarationOnly",
    "format": "npx @biomejs/biome format --write src/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version",
    "release": "npm run build && npm publish",
    "visualise": "npx vite-bundle-visualizer -o ./tmp/bundle-visualizer.html"
  },
  "author": "Joshua Wallace",
  "repository": {
    "type": "git",
    "url": "https://github.com/honedlabs/query-vue.git"
  },
  "homepage": "https://github.com/honedlabs/query-vue#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/query-vue.umd.cjs",
  "module": "./dist/query-vue.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/query-vue.js",
      "require": "./dist/query-vue.umd.cjs"
    }
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "@inertiajs/vue3": "^1.0.0||^2.0.0",
    "axios": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.9.3",
    "axios": "^1.2.0",
    "typescript": "^5.7.0",
    "vite": "^5.4.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  }
}